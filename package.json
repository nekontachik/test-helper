{
  "name": "next-test-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm run format:check",
    "validate:fix": "npm run lint:fix && npm run format",
    "check": "tsc --noEmit && eslint . --ext .ts,.tsx",
    "check:structure": "[ ! -d \"app\" ] || (echo \"Error: /app directory exists - use /src/app instead\" && exit 1)",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "deploy:staging": "vercel",
    "deploy:production": "vercel --prod",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "migrate:auth": "ts-node --project scripts/tsconfig.json scripts/migrate-auth.ts",
    "cleanup:auth": "ts-node --project scripts/tsconfig.json scripts/cleanup-auth.ts"
  },
